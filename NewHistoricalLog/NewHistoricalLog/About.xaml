<dx:ThemedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf"
    x:Class="NewHistoricalLog.About"
    WindowStartupLocation="CenterOwner"
    Title="О программе" Height="600" Width="700"
    ResizeMode="NoResize" Icon="{dx:DXImage Image=Info_32x32.png}" KeyDown="ThemedWindow_KeyDown" Topmost="True">
    <Grid>

        <dxpdf:PdfViewerControl x:Name="pdfViewer" CommandBarStyle="None" Grid.ColumnSpan="2" ShowStartScreen="False">
            <dxpdf:PdfViewerControl.ThumbnailsViewerSettings>
                <dxpdf:PdfThumbnailsViewerSettings x:Name="settings1" HideThumbnailsViewer="True"/>
            </dxpdf:PdfViewerControl.ThumbnailsViewerSettings>
            <dxpdf:PdfViewerControl.AttachmentsViewerSettings>
                <dxpdf:PdfAttachmentsViewerSettings x:Name="settings2" HideAttachmentsViewer="True"/>
            </dxpdf:PdfViewerControl.AttachmentsViewerSettings>
            <dxpdf:PdfViewerControl.OutlinesViewerSettings>
                <dxpdf:PdfOutlinesViewerSettings x:Name="settings3" HideOutlinesViewer="True"/>
            </dxpdf:PdfViewerControl.OutlinesViewerSettings>
        </dxpdf:PdfViewerControl>

    </Grid>
</dx:ThemedWindow>
